  "scripts": {
    "dev": "rm -rf dist & nx run-many --target=serve --parallel=20",
    "g-gateway": "nx g @nx/nest:app $name --directory gateways",
    "g-app": "nx g @nx/nest:app $name --directory microservices",
    "g-lib": "nx g @nx/nest:lib $name",
    "rm-app": "nx g @nx/workspace:remove $name --projectType=app",
    "lint": "nx run-many --target=lint --all",
    "typeorm": "typeorm-ts-node-esm",
    "g-migration": "typeorm-ts-node-esm migration:generate ./database/migrations/migration -d ./database/typeorm.config.ts",
    "c-migration": "typeorm-ts-node-esm migration:create ./database/migrations/migration",
    "r-migration": "typeorm-ts-node-esm migration:run -d ./database/typeorm.config.ts",
    "serve": "nx serve"
  },
  "private": true,
